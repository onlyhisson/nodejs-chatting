<div class="container-fluid">
    <div class="row">
        <div class="col-lg-12">
            <div class="view-header">
                <div class="pull-right text-right" style="line-height: 14px">
                    <small><%=pageTitle%><br>General<br> <span class="c-white">Table styles</span></small>
                </div>
                <div class="header-icon">
                    <i class="pe page-header-icon pe-7s-menu"></i>
                </div>
                <div class="header-title">
                    <h3><%=pageTitle%></h3>
                    <small><%=subTitle%></small>
                </div>
            </div>
            <hr>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-filled">
                <div class="panel-heading">
                    <div class="panel-tools">
                        <a class="panel-toggle"><i class="fa fa-chevron-up"></i></a>
                        <a class="panel-close"><i class="fa fa-times"></i></a>
                    </div>
                    채팅방 생성
                </div>
                <div class="panel-body">
                    <form action='' method="post">
                        <div class="form-group">
                            <label for="exampleInputName">방 이름</label> 
                            <input type="text" class="form-control" name="title" placeholder="Name">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputEmail1">제한 인원</label> 
                            <input type="number" class="form-control" name="max" placeholder="Email" min='2' value='10'>
                        </div>
                        <div class="form-group">
                            <label for="exampleInputPassword1">비밀번호</label> 
                            <input type="password" class="form-control" name="password" placeholder="Password">
                            <p class="form-text">비밀번호 없을 경우 공개방</p>
                        </div>
                        <!--
                        <div>
                            <label> 
                                <input type="checkbox"> Check me out 
                            </label>
                        </div>
                        -->
                        <button type="submit" class="btn btn-default">생성</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>